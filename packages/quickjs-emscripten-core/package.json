{
  "name": "quickjs-emscripten-core",
  "version": "0.25.1",
  "sideEffects": false,
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "eval",
    "quickjs",
    "vm",
    "interpreter",
    "runtime",
    "safe",
    "emscripten",
    "wasm"
  ],
  "author": {
    "name": "Jake Teton-Landis",
    "url": "https://jake.tl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/justjake/quickjs-emscripten"
  },
  "files": [
    "dist/**/*",
    "!dist/*.test.js",
    "!dist/*.tsbuildinfo"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "tsc": "npx tsc -p . --noEmit",
    "test": "npx vitest run",
    "build": "npx tsup"
  },
  "dependencies": {
    "@jitl/quickjs-ffi-types": "workspace:*"
  }
}
